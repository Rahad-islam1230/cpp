#include <iostream>

using namespace std;


int main() {
    int grade = 75;

    grade >= 60 ? std::cout << "You pass!": std::cout << "You fail!";



    return 0;


}
#include <iostream>

using namespace std;


int main() {
    int number = 9;
    number % 2 == 1 ? cout << "ODD" : cout << "EVEN";

    bool hungry = true;

    hungry ? cout << "your are hungry" : cout << "You are full";


    return 0;


}
